{
  "name": "luchnos",
  "version": "1.0.0",
  "description": "Lampe Allumée (Luchnos) - Site web ministère chrétien",
  "scripts": {
    "install-all": "cd backend && npm install && cd ../frontend && npm install",
    "dev-backend": "cd backend && npm run dev",
    "dev-frontend": "cd frontend && npm run dev",
    "start-backend": "cd backend && npm start",
    "start-frontend": "node serve-frontend.js",
    "build-frontend": "cd frontend && npm run build",
    "build:render": "node build-render.js",
    "render-build": "npm install && cd backend && npm install",
    "render-start": "cd backend && npm start",
    "frontend-build": "npm install && node build-render.js",
    "frontend-start": "node serve-frontend.js"
  },
  "keywords": [
    "luchnos",
    "ministere",
    "chretien",
    "lampe-allumee"
  ],
  "author": "Luchnos Ministries",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "express": "^4.22.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "canvas": "^3.2.0"
  }
}
